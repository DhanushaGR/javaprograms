public class Prime {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int n = 30 ;
		boolean a =false;
		for (int i =5;i<n/2;++i)
				{
			if(n/i==0) {
				
				a=true;
			break;
				}
	}
	package practicep;

public class Factory {
   int x=10;// instance member variable
   static int z=90;
 static void m1()  //class member function
{
	System.out.println("hi bindhu");
}
 void m2() // instance member function
 {
	 System.out.println("hi dhanusha");
 }

	public static void main(String[] args) {
		Factory f1 = new Factory(); //  object 
		System.out.println(f1.x);// x =10
		System.out.println(z);
		Factory.m1();
		f1.m2();
		
	} 

}
	
